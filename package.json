{
  "name": "next-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "analyze": "cross-env ANALYZE=true next build",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./ && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./",
    "lint:prettier": "check-prettier lint",
    "lint:style": "stylelint --fix \"/**/*.less\" --syntax less",
    "prettier": "prettier -c --write \"**/*\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:push": "cross-env CI=true npm test -- --noStackTrace --coverage=false --watchAll=false --verbose=false --testTimeout=20000",
    "view:coverage": "open coverage/lcov-report/index.html",
    "typecheck": "tsc --project jsconfig.json --noEmit --skipLibCheck"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged",
      "pre-push": "npm run typecheck && npm run test:push"
    }
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 65,
        "functions": 65,
        "lines": 75,
        "statements": 75
      }
    },
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/.next/",
      "((\\.| /)(stories|story|mock))\\.[jt]sx?$",
      "<rootDir>/.next",
      "<rootDir>/models",
      "<rootDir>/services",
      "<rootDir>/utils"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@ant-design/icons": "4.5.0",
    "@next/bundle-analyzer": "10.0.7",
    "@zeit/next-less": "1.0.1",
    "@zeit/next-sass": "1.0.1",
    "antd": "4.13.0",
    "antd-dayjs-webpack-plugin": "1.0.6",
    "babel-plugin-import": "1.13.3",
    "cross-env": "7.0.3",
    "dayjs": "1.10.4",
    "dotenv": "8.2.0",
    "dva-loading": "3.0.22",
    "dva-no-router": "1.2.1",
    "express": "4.17.1",
    "http-proxy-middleware": "1.0.6",
    "less": "4.1.1",
    "less-vars-to-js": "1.3.0",
    "next": "10.0.7",
    "next-purgecss": "4.0.0",
    "next-seo": "4.20.0",
    "nextjs-sitemap-generator": "1.3.1",
    "null-loader": "4.0.1",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "styled-components": "5.2.1",
    "umi-request": "1.3.5",
    "webpack-filter-warnings-plugin": "1.2.1"
  },
  "devDependencies": {
    "@babel/core": "7.13.8",
    "@babel/plugin-proposal-decorators": "7.13.5",
    "@typescript-eslint/eslint-plugin": "4.16.1",
    "@umijs/fabric": "2.5.7",
    "babel-core": "6.26.3",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.6.3",
    "babel-plugin-module-resolver": "4.1.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "check-prettier": "1.0.3",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.6",
    "eslint": "7.21.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-prettier": "8.1.0",
    "eslint-import-resolver-babel-module": "5.2.0",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.1.5",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-unicorn": "28.0.2",
    "husky": "4.3.8",
    "jest": "26.6.3",
    "jest-enzyme": "7.1.2",
    "lint-staged": "10.5.4",
    "prettier": "2.2.1",
    "stylelint": "13.11.0"
  },
  "engines": {
    "node": ">=10.0.0"
  }
}
